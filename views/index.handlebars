<h1>burgers!</h1>
<h2>burgers that are not is_eaten!</h2>
<ul>
  {{#each burgers}}
    {{#unless devoured}}
      <span><strong>{{this.id}} - {{this.burger_name}}</strong></span>
      <button type="button" class="eat_burger" id ="eat_button" data-id="{{this.id}}" data-newDevoured="{{devoured}}"></button>
    {{/unless}}
  {{/each}}
</ul>

<form class="create-form">
  <div class="form-group">
    <label for="newburger">burger Name:</label>
    <input type="text" id="newburger" name="burger">
  </div>
  <div class="form-group">
    <label for="is_eaten">is_eaten?</label><br>
  </div>
  <button type="submit" id="addburger">Add burger</button>
</form>